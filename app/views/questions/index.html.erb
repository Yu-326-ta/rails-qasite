<h1>Question</h1>
<table>
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Name</th>
        <th>Content</th>
        <th></th>
    </tr>
    <% @questions.each do |question| %>
    <tr>
        <th><%= question.id %></th>
        <th><%= question.title %></th>
        <th><%= question.name %></th>
        <th><%= question.content %></th>
        <th><%= link_to 'show', question_path(question) %></th>
    </tr>
    <% end %>
</table>